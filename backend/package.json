    "passport-facebook-token": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "sharp": "^0.28.3",
    "slugify": "^1.5.3",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2"
